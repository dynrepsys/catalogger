<h1>New File</h1>
<%= form_for :file, url: files_path do |f| %>
  <p>
    <%= f.label :md5 %><br>
    <%= f.text_field :md5 %>
  </p>

  <p>
    <%= f.label :path %><br>
    <%= f.text_field :path %>
  </p>

  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
